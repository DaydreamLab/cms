{
    "name": "daydreamlab/cms",
    "description": "DaydreamLab CMS",
    "authors": [
        {
            "name": "Jordan Tsai",
            "email": "jordan@daydream-lab.com"
        }
    ],
    "autoload": {
        "classmap": [
        ],
        "psr-4": {
            "App\\": "app/",
            "DaydreamLab\\Cms\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "require": {
        "daydreamlab/jjaj": "dev-master",
        "daydreamlab/user": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "repositories" : [
        {
            "type": "vcs",
            "url":  "git@github.com:DaydreamLab/JJAJ.git",
            "no-api": true
        },
        {
            "type": "vcs",
            "url":  "git@github.com:DaydreamLab/user.git",
            "no-api": true
        },
        {
            "type": "vcs",
            "url":  "git@github.com:DaydreamLab/media.git",
            "no-api": true
        },
        {
            "type": "vcs",
            "url":  "git@github.com:DaydreamLab/observer.git",
            "no-api": true
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "DaydreamLab\\Cms\\CmsServiceProvider"
            ]
        }
    }
}
