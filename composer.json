{
    "name": "daydreamlab/cms",
    "description": "DaydreamLab CMS",
    "authors": [
        {
            "name": "Jordan Tsai",
            "email": "jordan@daydream-lab.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "DaydreamLab\\Cms\\": "src/",
            "DaydreamLab\\Cms\\Database\\Seeders\\": "database/seeders",
            "DaydreamLab\\Cms\\Database\\Factories\\": "database/factories",
            "DaydreamLab\\Cms\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "require": {
        "daydreamlab/user": "dev-v2/main",
        "daydreamlab/media": "dev-v2/main",
        "fedeisas/laravel-mail-css-inliner": "^4.0"
    },
    "repositories" : [
        {
            "type": "vcs",
            "url":  "https://github.com/DaydreamLab/JJAJ.git"
        },
        {
            "type": "vcs",
            "url":  "https://github.com/DaydreamLab/user.git"
        },
        {
            "type": "vcs",
            "url":  "https://github.com/DaydreamLab/media.git"
        },
        {
            "type": "vcs",
            "url":  "https://github.com/DaydreamLab/observer.git"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "extra": {
        "laravel": {
            "providers": [
                "DaydreamLab\\Cms\\CmsServiceProvider"
            ]
        }
    }
}
